{
    "extends": ["tslint:all"],
    "rules": {
        "typedef": [
            true,
            "call-signature",
            "arrow-call-signature",
            "parameter",
            "arrow-parameter",
            "property-declaration",
            "member-variable-declaration",
            "object-destructuring",
            "array-destructuring"
        ],
        "no-require-imports": false,
    }
}